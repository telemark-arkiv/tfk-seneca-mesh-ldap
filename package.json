  },
  "keywords": [
    "ldap",
    "group",
    "tfk",
    "Telemark fylkeskommune"
  ],
  "repository": {
    "type": "git",
    "url": "http://github.com/telemark/tfk-seneca-mesh-ldap.git"
  },
  "bugs": {
